{
  "name": "tvpine-cli",
  "version": "1.0.0",
  "description": "Local Pine Script runner with free exchange data - TradingView alternative",
  "main": "src/cli.js",
  "bin": {
    "tvpine": "./src/cli.js"
  },
  "type": "module",
  "scripts": {
    "dev": "node src/cli.js",
    "test": "node src/cli.js scripts/test.pine --symbol BTCUSDT --timeframe 1m",
    "stream": "python3 src/datafeed/streamer.py",
    "stream:1s": "python3 src/datafeed/streamer.py --timeframe 1s",
    "stream:1m": "python3 src/datafeed/streamer.py --timeframe 1m",
    "backtest": "node src/cli.js --backtest"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.16.0",
    "canvas": "^2.11.2",
    "commander": "^12.0.0",
    "csv-parse": "^5.5.0",
    "csv-stringify": "^6.4.0"
  },
  "devDependencies": {
    "vitest": "^1.4.0"
  },
  "keywords": ["pine-script", "tradingview", "trading", "crypto", "charting", "cli"],
  "author": "Moon AI Trading Platform",
  "license": "MIT"
}
